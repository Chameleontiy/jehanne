{
	"KernelLibs": {
		"Cflags": [
			"-std=c11",
			"-c",
			"-I/$JEHANNE/arch/$ARCH/include",
			"-I/$JEHANNE/sys/include",
			"-I.",
			"-mcmodel=kernel",
			"-O0",
			"-fplan9-extensions",
			"-mno-red-zone",
			"-ffreestanding",
			"-fno-builtin",
			"-Wall",
			"-Wno-missing-braces",
			"-Wno-parentheses",
			"-Wno-unknown-pragmas",
			"-Wuninitialized",
			"-g"
		],
		"Oflags": [
			"-static"
		],
		"Post": [
			"rm *.o"
		],
		"Pre": [
			"rm -f *.o *.tag.*"
		]
	}
}
